{
  "name": "@folio/checkin",
  "version": "6.0.0",
  "description": "Item Check-in",
  "repository": "folio-org/ui-checkin",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "src/index.js",
  "stripes": {
    "actsAs": [
      "app"
    ],
    "displayName": "ui-checkin.meta.title",
    "route": "/checkin",
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "Check in items",
        "title": "Checkout"
      }
    ],
    "okapiInterfaces": {
      "circulation": "9.0 10.0 11.0 12.0 13.0",
      "configuration": "2.0",
      "inventory": "10.0 11.0",
      "loan-policy-storage": "1.0 2.0",
      "users": "15.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.checkin.enabled",
        "displayName": "UI: Check in module is enabled"
      },
      {
        "permissionName": "ui-checkin.all",
        "displayName": "Check in: All permissions",
        "description": "Entire set of permissions needed to use Checkin",
        "visible": true,
        "subPermissions": [
          "users.collection.get",
          "usergroups.collection.get",
          "module.checkin.enabled",
          "inventory.items.collection.get",
          "inventory-storage.service-points.collection.get",
          "accounts.collection.get",
          "accounts.item.get",
          "lost-item-fees-policies.collection.get",
          "circulation.check-in-by-barcode.post",
          "circulation.requests.collection.get",
          "feefineactions.collection.get",
          "configuration.entries.collection.get",
          "circulation.end-patron-action-session.post",
          "circulation-storage.staff-slips.collection.get"
        ]
      }
    ]
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build",
    "test": "yarn run test:jest && yarn run test:bigtest ",
    "test:bigtest": "stripes test karma",
    "test:jest": "jest --ci --coverage --colors",
    "lint": "eslint .",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-checkin ./translations/ui-checkin/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@bigtest/interactor": "^0.7.0",
    "@bigtest/mocha": "^0.5.0",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.1.0",
    "@folio/stripes-cli": "^2.5.0",
    "@folio/stripes-components": "^10.0.0",
    "@folio/stripes-core": "^8.0.0",
    "@formatjs/cli": "^4.2.20",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.1.2",
    "core-js": "^3.6.4",
    "eslint": "^7.32.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "inflected": "^2.0.4",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "miragejs": "^0.1.40",
    "mocha": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.13.9",
    "sinon": "^6.3.4"
  },
  "dependencies": {
    "@folio/react-intl-safe-html": "^3.1.0",
    "dateformat": "^2.0.0",
    "final-form": "^4.19.1",
    "html-to-react": "^1.3.3",
    "inactivity-timer": "^1.0.0",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "react-barcode": "^1.3.2",
    "react-final-form": "^6.4.0",
    "react-hot-loader": "^4.3.12",
    "react-to-print": "^2.3.2"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-intl": "^5.7.0",
    "react-router-dom": "^5.2.0"
  }
}
