{
  "name": "@folio/checkin",
  "version": "1.3.1",
  "description": "Item Check-in",
  "repository": "folio-org/ui-checkin",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "src/index.js",
  "stripes": {
    "type": "app",
    "displayName": "ui-checkin.meta.title",
    "route": "/checkin",
    "queryResource": "query",
    "hasSettings": false,
    "icons": [
      {
        "name": "app",
        "alt": "Check in items",
        "title": "Checkout"
      }
    ],
    "okapiInterfaces": {
      "circulation": "3.0 4.0 5.0",
      "configuration": "2.0",
      "item-storage": "5.0 6.0",
      "loan-policy-storage": "1.0",
      "users": "15.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.checkin.enabled",
        "displayName": "UI: Check in module is enabled"
      },
      {
        "permissionName": "ui-checkin.all",
        "displayName": "Check in: All permissions",
        "description": "Entire set of permissions needed to use Checkin",
        "visible": true,
        "subPermissions": [
          "circulation.all",
          "circulation-storage.all",
          "configuration.all",
          "users.collection.get",
          "module.checkin.enabled"
        ]
      }
    ]
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build",
    "test": "stripes test karma",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.7.0",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.0",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^3.2.1",
    "@folio/stripes": "^1.0.0",
    "@folio/stripes-cli": "^1.4.0",
    "@folio/stripes-core": "^2.15.1",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "mocha": "^5.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "sinon": "^6.3.4",
    "webpack": "^4.16.0"
  },
  "dependencies": {
    "@folio/react-intl-safe-html": "^1.0.0",
    "dateformat": "^2.0.0",
    "html-to-react": "^1.3.3",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "react-barcode": "^1.3.2",
    "react-intl": "^2.4.0",
    "react-router-dom": "^4.0.0",
    "react-to-print": "^1.0.21",
    "redux-form": "^7.0.3"
  },
  "peerDependencies": {
    "@folio/stripes": "^1.0.0",
    "react": "*"
  }
}
