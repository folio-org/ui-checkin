{
  "name": "@folio/checkin",
  "version": "1.1.1",
  "description": "Item Check-in",
  "repository": "folio-org/ui-checkin",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "stripes": {
    "type": "app",
    "displayName": "Check in",
    "route": "/checkin",
    "hasSettings": false,
    "icons": [
     {
       "name": "app",
       "alt": "Check in items",
       "title": "Checkout"
     }
    ],
    "okapiInterfaces": {
      "circulation": "2.2",
      "configuration": "2.0",
      "item-storage": "5.0",
      "loan-policy-storage": "1.0",
      "users": "14.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.checkin.enabled",
        "displayName": "UI: Check in module is enabled"
      },
      {
        "permissionName": "ui-checkin.all",
        "displayName": "Check in: All permissions",
        "description": "Entire set of permissions needed to use Checkin",
        "visible": true,
        "subPermissions": [
          "circulation.all",
          "circulation-storage.all",
          "configuration.all",
          "users.collection.get",
          "module.checkin.enabled"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint *.js",
    "test": "echo 'placeholder. no tests implemented'"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^1.1.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint": "^4.8.0"
  },
  "dependencies": {
    "@folio/stripes-components": "^2.0.0",
    "dateformat": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react-router-dom": "^4.0.0",
    "redux-form": "^7.0.3",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "@folio/stripes-connect": "^2.2.1",
    "@folio/stripes-core": "^2.1.0",
    "react": "*"
  }
}
